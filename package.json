{
  "name": "instantvisio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android-dev": "SET ENVFILE=.env.dev && react-native run-android",
    "android-test": "SET ENVFILE=.env.test && react-native run-android",
    "android-preprod": "SET ENVFILE=.env.preprod && react-native run-android",
    "android-prod": "SET ENVFILE=.env.prod && react-native run-android",
    "ios:dev": "export ENVFILE=.env.dev && react-native run-ios --scheme ENV",
    "ios:test": "export ENVFILE=.env.test && react-native run-ios --scheme ENV",
    "ios:preprod": "export ENVFILE=.env.preprod && react-native run-ios --scheme ENV",
    "ios:prod": "export ENVFILE=.env.prod && react-native run-ios --scheme ENV",
    "mac:android-dev": "export ENVFILE=.env.dev && react-native run-android",
    "mac:android-test": "export ENVFILE=.env.test && react-native run-android",
    "mac:android-preprod": "export ENVFILE=.env.preprod && react-native run-android",
    "mac:android-prod": "export ENVFILE=.env.prod && react-native run-android",
    "mac:build-android-test": "export ENVFILE=.env.test && cd android && ./gradlew assembleDebug && cd ..",
    "mac:build-android-preprod": "export ENVFILE=.env.preprod && cd android && ./gradlew assembleRelease && cd ..",
    "mac:build-android-prod": "export ENVFILE=.env.prod && cd android && ./gradlew assembleRelease && cd ..",
    "build-android-test": "SET ENVFILE=.env.test && cd android && gradlew assembleRelease && cd ..",
    "build-android-preprod": "SET ENVFILE=.env.preprod && cd android && gradlew assembleRelease && cd ..",
    "build-android-prod": "SET ENVFILE=.env.prod && cd android && gradlew assembleRelease && cd ..",
    "build-android-prod-aab": "SET ENVFILE=.env.prod && cd android && gradlew bundleRelease && cd .."
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-firebase/analytics": "^6.3.4",
    "@react-native-firebase/app": "^6.3.4",
    "@react-native-firebase/auth": "^6.3.4",
    "@react-native-firebase/functions": "^6.3.4",
    "@react-native-firebase/remote-config": "^6.3.4",
    "crypto-js": "^3.1.9-1",
    "immutable": "3.8.2",
    "native-base": "2.12.1",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-config": "0.12.0",
    "react-native-gesture-handler": "^1.1.0",
    "react-native-i18n": "2.0.12",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal-dropdown": "^0.7.0",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.6.4",
    "react-native-screens": "^2.0.0-alpha.34",
    "react-native-snap-carousel": "^3.8.4",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.4",
    "react-navigation-drawer": "^2.3.3",
    "react-navigation-stack": "^2.1.1",
    "react-navigation-tabs": "^2.7.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "sendgrid": "^5.2.3",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
